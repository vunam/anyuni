{
  "root": true,
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [ "babel" ],
  "rules": {
    "comma-dangle": 0,
    "dot-notation": 0,
    "arrow-parens": 0,
    "no-param-reassign": 0,
    "semi": 0,
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "browser": true
  }
}
